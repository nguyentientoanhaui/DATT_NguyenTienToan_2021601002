@model Shopping_Demo.Models.ContactModel

@if (Model != null)
{
    <div style="display:flex; align-items:end">
        <img src="~/media/logo/@Model.LogoImage" width="60" height="60" style="border-radius: 50%;filter: brightness(0) invert(1);" />
        <p class="sidebar-brand-text" style="font-weight: bold; color: white; font-size:24px;">
            @Model.Name
        </p>
    </div>
}